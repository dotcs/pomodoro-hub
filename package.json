{
  "name": "pomodoro-hub",
  "private": true,
  "version": "0.1.0",
  "main": "build/main.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "webpack --watch",
    "start": "electron .",
    "lint":
      "tslint --type-check --project tsconfig.json --config tslint.json \"src/**/*.{ts,tsx}\"",
    "build": "webpack",
    "package:osx":
      "electron-packager ./ --platform=darwin --arch=x64 --out=out --overwrite",
    "prettier:base": "prettier --parser typescript",
    "prettier:list":
      "yarn prettier:base --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base --write \"src/**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "css-loader": "^0.28.11",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^12.0.0",
    "electron-prebuilt": "^1.4.13",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "electron": "^1.8.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.41",
    "material-ui-icons": "^1.0.0-beta.36",
    "node-noop": "^1.0.0",
    "normalize.css": "^8.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
